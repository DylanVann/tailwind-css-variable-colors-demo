{
  "name": "tailwind-variable-color-demo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "run-s tailwind:build react-scripts:build",
    "eject": "react-scripts eject",
    "react-scripts:build": "react-scripts build",
    "react-scripts:dev": "react-scripts start",
    "start": "run-p tailwind:dev react-scripts:dev",
    "tailwind:build": "NODE_ENV=production postcss src/index.postcss.css -o src/index.css",
    "tailwind:dev": "postcss src/index.postcss.css -o src/index.css -w",
    "test": "react-scripts test"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "autoprefixer": "^10.0.2",
    "cssnano": "^4.1.10",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.10",
    "postcss-cli": "^8.3.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.1",
    "tailwindcss": "^2.0.1",
    "web-vitals": "^0.2.4"
  }
}
